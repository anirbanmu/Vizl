{
  "name": "vizl",
  "version": "1.0.0",
  "license": "MIT",
  "engines": {
    "node": "^16.13",
    "yarn": "^1.22"
  },
  "scripts": {
    "build": "rollup -c && yarn run build-server",
    "dev": "rollup -c -w",
    "start": "node server.js",
    "validate": "svelte-check",
    "build-server": "tsc --target es6 --module commonjs --esModuleInterop true --strict true server.ts",
    "start-dev": "tsc --target es6 --module commonjs --esModuleInterop true --strict true server.ts && node server.js"
  },
  "devDependencies": {
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.1.1",
    "@rollup/plugin-typescript": "^8.3.0",
    "@tsconfig/svelte": "^3.0.0",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.13",
    "@types/express-sslify": "^1.2.2",
    "@types/ioredis": "^4.28.5",
    "rollup": "^2.62.0",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.0",
    "svelte": "^3.44.3",
    "svelte-check": "^2.2.11",
    "svelte-fa": "^2.4.0",
    "svelte-preprocess": "^4.10.1",
    "tslib": "^2.3.1",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "axios": "^0.24.0",
    "compression": "^1.7.4",
    "express": "^4.17.2",
    "express-sslify": "^1.2.0",
    "ioredis": "^4.28.2"
  }
}
