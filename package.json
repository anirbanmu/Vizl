{
  "name": "vizl",
  "version": "1.0.0",
  "license": "MIT",
  "engines": {
    "node": "^18.14"
  },
  "scripts": {
    "build": "rollup -c && yarn run build-server",
    "dev": "rollup -c -w",
    "start": "node server.js",
    "validate": "svelte-check",
    "build-server": "tsc --target es6 --module commonjs --esModuleInterop true --strict true server.ts",
    "start-dev": "tsc --target es6 --module commonjs --esModuleInterop true --strict true server.ts && node server.js"
  },
  "devDependencies": {
    "@fortawesome/free-solid-svg-icons": "^6.1.2",
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-terser": "^0.4.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "@tsconfig/svelte": "^3.0.0",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.13",
    "@types/express-sslify": "^1.2.2",
    "rollup": "^3.18.0",
    "rollup-plugin-css-only": "^4.3.0",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-svelte": "^7.1.4",
    "svelte": "^3.49.0",
    "svelte-check": "^2.8.1",
    "svelte-fa": "^3.0.3",
    "svelte-preprocess": "^4.10.7",
    "tslib": "^2.4.0",
    "typescript": "^4.8.2"
  },
  "dependencies": {
    "axios": "^1.3.4",
    "compression": "^1.7.4",
    "express": "^4.18.1",
    "express-sslify": "^1.2.0",
    "keyv": "^4.5.2"
  }
}
