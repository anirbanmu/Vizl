{
  "name": "vizl",
  "version": "1.0.0",
  "license": "MIT",
  "engines": {
    "node": "^15.14",
    "yarn": "^1.22"
  },
  "scripts": {
    "build": "rollup -c && yarn run build-server",
    "dev": "rollup -c -w",
    "start": "node server.js",
    "validate": "svelte-check",
    "build-server": "tsc --target es6 --module commonjs --esModuleInterop true --strict true server.ts",
    "start-dev": "tsc --target es6 --module commonjs --esModuleInterop true --strict true server.ts && node server.js"
  },
  "devDependencies": {
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@rollup/plugin-typescript": "^8.2.5",
    "@tsconfig/svelte": "^2.0.1",
    "@types/compression": "^1.7.1",
    "@types/express": "^4.17.13",
    "@types/express-sslify": "^1.2.2",
    "@types/ioredis": "^4.26.7",
    "rollup": "^2.56.2",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.0",
    "svelte": "^3.42.1",
    "svelte-check": "^2.2.4",
    "svelte-fa": "^2.2.1",
    "svelte-preprocess": "^4.7.4",
    "tslib": "^2.3.1",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "compression": "^1.7.4",
    "express": "^4.17.1",
    "express-sslify": "^1.2.0",
    "ioredis": "^4.27.7"
  }
}
